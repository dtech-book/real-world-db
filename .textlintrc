{
  "plugins": [
    "review"
  ],
  "filters": {},
  "rules": {
    "preset-ja-technical-writing": true,
    "preset-japanese": true,
    "textlint-rule-ja-yahoo-kousei": {
      "ignores": {
        "用字": ["カラム", "作成", "特性", "作成", "最小", "箇所", "以外", "指向", "特徴", "移動", "保証", "決済", "改定", "割引"],
        "助詞不足の可能性あり": ["マスタレコード", "スキーマレス", "モデル設計", "アプリケーションレベル", "SQL", "取得可能", "JOINする", "取得する", "データベースレベル", "担保する", "受注明細"]
      }
    },
    "max-ten": {
      "max": 5
    }
  }
}
